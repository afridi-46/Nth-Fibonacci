        "n = int(input(\"Enter input number : \"))\n",
        "def Fibo(n):\n",
        "    if n<= 0:\n",
        "        print(\"Incorrect input\")\n",
        "\n",
        "    elif n == 1:\n",
        "        return 0\n",
        "\n",
        "    elif n == 2:\n",
        "        return 1\n",
        "    else:\n",
        "        return Fibo(n-1)+Fibo(n-2)\n",
        "\n",
        "\n",
        "print(Fibo(n))"
